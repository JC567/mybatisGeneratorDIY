    <classPathEntry location="D://Maven//repo//mysql//mysql-connector-java//8.0.11//mysql-connector-java-8.0.11.jar"/>
<!--    <classPathEntry location="D://Maven//repo//mysql//mysql-connector-java//5.1.47//mysql-connector-java-5.1.47.jar"/>-->
    <!--<classPathEntry location="D://Program Files//instantclient_12_1//ojdbc6.jar"/>-->

    <context id="context" targetRuntime="MyBatis3" defaultModelType="flat">

        <!-- 指定生成的java文件的编码,没有直接生成到项目时中文可能会乱码 -->
        <property name="javaFileEncoding" value="UTF-8"/>
        <commentGenerator type="com.jc.MyCommentGenerator">
        </commentGenerator>

        <!-- 数据库的相关配置 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://10.20.32.76:3306/ztlpyjy2020?useUnicode=true&amp;characterEncoding=utf-8" userId="root" password="Caifu123">
<!--        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://10.20.26.247:33061/zyt_liuyl?useUnicode=true&amp;characterEncoding=utf-8" userId="root" password="mysql">-->
            <property name="useInformationSchema" value="true"></property>
        </jdbcConnection>
        <!--<jdbcConnection driverClass="oracle.jdbc.driver.OracleDriver" connectionURL="jdbc:oracle:thin:@10.20.35.217:1521:orcl" userId="finiia" password="finiia"/>-->

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <!-- 实体类生成的位置 -->
        <javaModelGenerator targetPackage="com.jc.pojo" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <!-- *Mapper.xml 文件的位置 -->
        <sqlMapGenerator targetPackage="com.jc.mapper" targetProject="src/main/resources">
            <property name="enableSubPackages" value="false"/>
        </sqlMapGenerator>

        <!-- Mapper 接口文件的位置 -->
        <javaClientGenerator targetPackage="com.jc.dao" targetProject="src/main/java" type="XMLMAPPER">
            <property name="enableSubPackages" value="false"/>

        </javaClientGenerator>
        <!-- 相关表的配置 -->
        <table tableName="TC_TREQUEST" domainObjectName="Request1"
               enableCountByExample="false" enableDeleteByExample="false" enableSelectByExample="false"
               enableUpdateByExample="false"
               enableSelectByPrimaryKey="true" enableUpdateByPrimaryKey="true" enableDeleteByPrimaryKey="true" >
<!--            <domainObjectRenamingRule searchString="^TcT" replaceString="" />-->
            <columnRenamingRule searchString="^[^_]+" replaceString=""/>
        </table>
    </context>
</generatorConfiguration>
